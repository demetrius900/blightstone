      "BACKEND_API_URL": "https://adhub-backend-prod.onrender.com",
      "NEXT_PUBLIC_SUPABASE_URL": "https://njtwgqucvrbvhqqhycxe.supabase.co",
      "NEXT_PUBLIC_SUPABASE_ANON_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6Im5qdHdncXVjdnJidmhxcWh5Y3hlIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NDkwNDYyMDcsImV4cCI6MjA2NDYyMjIwN30.bSA4n2PnMrJX1Izdn9ct4tuUSSNsA0KHyeYEXcKdSkI",
      "NEXT_PUBLIC_API_URL": "http://localhost:8000"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/proxy/:path*",
      "destination": "/api/proxy/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "crons": []
} 