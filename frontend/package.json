{
  "name": "adhub-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run audit:production && next build",
    "build:unsafe": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='__tests__' --testPathIgnorePatterns='integration|e2e'",
    "test:integration": "jest --testPathPattern='integration|workflow'",
    "test:security": "jest --testPathPattern='security-validation'",
    "test:financial": "jest --testPathPattern='financial|wallet|production-safety'",
    "test:api": "jest --testPathPattern='api-integration'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:financial && npm run test:api",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "playwright test performance-benchmarks.spec.ts",
    "test:full": "npm run test:all && npm run test:e2e && npm run test:performance",
    "test:ci": "npm run lint && npm run type-check && npm run test:coverage && npm run build:unsafe && npm run test:e2e",
    "test:quick": "npm run test:unit && npm run lint",
    "test:financial-safety": "npm run test:financial && npm run test:security",
    "precommit": "npm run test:quick",
    "prepush": "npm run test:ci",
    "audit:production": "node scripts/production-readiness-audit.js",
    "audit:production:report": "npm run audit:production && echo 'Report saved to PRODUCTION_READINESS_REPORT.md'",
    "security:audit": "npm audit && node ../scripts/enhanced-security-dashboard.js",
    "security:fix": "node scripts/immediate-security-fixes.js",
    "security:check": "node ../scripts/enhanced-security-dashboard.js"
  },
