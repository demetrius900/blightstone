				{
					"name": "List browser profiles",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "https://anty-api.com/browser_profiles?limit&query&tags[]&statuses[]&mainWebsites[]&users[]&page",
							"protocol": "https",
							"host": [
								"anty-api",
								"com"
							],
							"path": [
								"browser_profiles"
							],
							"query": [
								{
									"key": "limit",
									"value": "",
									"description": "How many browser profiles to return per page. Max - 50. Default - 50."
								},
								{
									"key": "query",
									"value": "",
									"description": "Text search pattern by browser profile name."
								},
								{
									"key": "tags[]",
									"value": "",
									"description": "Array of tags in raw text, not tags' IDs. If you specify more than one tag this filter will work in AND mode. For example if you specify tags `facebook` and `italy` API will return only browser profiles which have BOTH tags."
								},
								{
									"key": "statuses[]",
									"value": "",
									"description": "Array of statuses IDs to filter with."
								},
								{
									"key": "mainWebsites[]",
									"value": "",
									"description": "Array of main website labels browser profiles were create with. Available values: `['facebook', 'google', 'vk', 'tiktok']`."
								},
								{
									"key": "users[]",
									"value": "",
									"description": "Array of users IDs to filter with. If not specifed only current user's browser profiles will be returned."
								},
								{
									"key": "page",
									"value": "",
									"description": "Page number"
								}
							]
						}
					},
					"response": []
				},
